<%= render 'shared/content_title', title: "New Category" %>

<div class="well span4">
  <%= render 'shared/error', obj: @category %>

  <%= form_for @category do |f| %>
    <div class: "control-group">
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>
    <%= f.submit class: "btn btn-success" %>
  <% end %>
</div>
